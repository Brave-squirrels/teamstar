{
  "name": "mystery",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/Brave-squirrels/mystery.git",
  "author": "Olek <aleksander.atamanczuk@gmail.com>",
  "license": "MIT",
  "scripts": {
    "client:i": "cd client && yarn install",
    "server:i": "cd server && yarn install",
    "current:i": "concurrently \"yarn client:i\" \"yarn server:i\"",
    "client": "cd client && yarn dev",
    "client:w": "cd client && yarn dev:w",
    "server": "cd server && yarn dev",
    "server:w": "cd server && yarn dev:w",
    "dev:i": "yarn install && yarn current:i",
    "dev": "concurrently \"yarn server\" \"yarn client\"",
    "dev:w": "concurrently \"yarn server:w\" \"yarn client:w\""
  },
  "devDependencies": {
    "concurrently": "^6.0.2"
  }
}
